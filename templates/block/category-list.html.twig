<h2>Категории</h2>
<div class="panel-group category-products" id="accordian">
  {% for category in categories %}
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a href="/category/{{ category.id }}">{{ category.title }}</a>
        </h4>
      </div>
    </div>
  {% endfor %}
</div>
